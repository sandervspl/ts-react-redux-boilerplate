{
  "name": "ts-react-redux-boilerplate",
  "version": "4.0.0",
  "description": "A starter boilerplate to create comprehensive React apps with TypeScript, Redux, Styled-Components.",
  "author": "sandervispoel <contact@sandervispoel.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/sandervspl/ts-react-boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development ts-node -r tsconfig-paths/register ./devServer.ts",
    "build": "NODE_ENV=production npm run clean && npm run build:js",
    "build:js": "webpack -p --hide-modules --config config/webpack/prod.ts",
    "clean": "rimraf dist/",
    "start": "node dist/server.js"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "redux",
    "boilerplate",
    "babel",
    "typescript",
    "styled-components"
  ],
